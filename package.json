{
    "name": "altarica-lang",
    "displayName": "AltaRica Lang",
    "description": "AltaRica lang is an vscode extension to support the `AltaRica` language",
    "version": "0.0.3",
    "icon": "images/icon.png",
    "author": "charles66820",
    "publisher": "charles66820",
    "engines": {
        "vscode": "^1.50.0"
    },
    "repository": { "type": "git", "url": "https://github.com/charles66820/altarica-vscode-extension.git" },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "alt",
            "aliases": ["AltaRica", "alt"],
            "extensions": [".alt", ".spe"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "alt",
            "scopeName": "source.alt",
            "path": "./syntaxes/alt.tmLanguage.json"
        }]
    }
}